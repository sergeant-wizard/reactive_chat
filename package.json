{
  "name": "reactive_chat",
  "repository": "https://github.com/sergeant-wizard/reactive_chat",
  "version": "0.0.0",
  "description": "chat inteface based on react",
  "main": "lib/index.js",
  "dependencies": {
    "bootstrap": "^3.3.5",
    "jquery": "^2.1.4",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "reactify": "^0.15.2",
    "watchify": "^2.1.1",
    "envify": "^3.0.0",
    "browserify": "^6.2.0",
    "babelify": "^6.3.0"
  },
  "scripts": {
    "start": "http-server",
    "watch": "watchify -t babelify -o js/bundle.js -v -d js/app.js",
    "build": "browserify -t babelify -o js/bundle.js js/app.js"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  },
  "author": "Ryo Miyajima"
}
